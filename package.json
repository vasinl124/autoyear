{
  "name": "@maxlim/autoyear",
  "version": "1.0.0",
  "description": "A React component that automatically renders the current year - perfect for copyright footers",
  "main": "dist/autoyear.cjs.js",
  "module": "dist/autoyear.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/autoyear.es.js",
      "require": "./dist/autoyear.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "year",
    "copyright",
    "footer",
    "date",
    "auto",
    "dynamic",
    "component"
  ],
  "author": "Max Limsukhawat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxlimsukhawat/autoyear.git"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0"
  }
}
